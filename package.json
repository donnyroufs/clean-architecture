{
  "name": "clean-todo",
  "version": "1.0.0",
  "main": "bootstrap.ts",
  "scripts": {
    "run:dev": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/bootstrap.ts",
    "make:migration": "knex migrate:make --migrations-directory ./src/drivers/database/data/migrations -x ts ${NAME}",
    "run:migration": "knex --knexfile ./src/knexfile.ts migrate:latest"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/knex": "^0.16.1",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.7",
    "class-transformer": "^0.3.1",
    "dotenv": "^8.2.0",
    "inversify": "^5.0.1",
    "nodemon": "^2.0.6",
    "reflect-metadata": "^0.1.13"
  },
  "dependencies": {
    "express": "^4.17.1",
    "inversify-express-utils": "^6.3.2",
    "knex": "^0.21.12",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
